<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<?import javafx.geometry.Insets?>

<AnchorPane fx:id="anchorPane" prefHeight="293.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="contentVBox" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets top="10.0" right="10.0" bottom="10.0" left="10.0"/>
         </padding>
         <children>
            <ImageView fx:id="logoImageView" fitHeight="80.0" preserveRatio="true" />
            <Label fx:id="titleLabel" alignment="CENTER" text="Choose connection Type" />
            <HBox fx:id="buttonsHBox" alignment="CENTER" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" fx:id="TCPButton" text="TCP" />
                  <Button mnemonicParsing="false" fx:id="RMIButton" text="RMI" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
