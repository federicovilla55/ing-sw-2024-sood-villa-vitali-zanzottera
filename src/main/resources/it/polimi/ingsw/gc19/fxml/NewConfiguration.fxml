<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="293.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="contentVBox" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <ImageView fx:id="logoImageView" fitHeight="80.0" preserveRatio="true" />
            <Label fx:id="titleLabel" alignment="CENTER" text="Choose connection Type" />
            <HBox fx:id="buttonsHBox" alignment="CENTER" spacing="20.0">
               <children>
                  <Button fx:id="TCPButton" mnemonicParsing="false" text="TCP" textAlignment="CENTER" />
                  <Button fx:id="RMIButton" mnemonicParsing="false" text="RMI" textAlignment="CENTER" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
