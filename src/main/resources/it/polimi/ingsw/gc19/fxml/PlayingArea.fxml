<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="stackPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css/SetupScene.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
            <children>
                <VBox fx:id="leftVBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="25.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                    <children>
                        <HBox fx:id="infoHBox" styleClass="infoHBox" stylesheets="@../css/PlayingAreaScene.css" alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" spacing="100.0" HBox.hgrow="ALWAYS" VBox.vgrow="NEVER">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
                <VBox fx:id="rightVBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="25.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
                    <children>
                        <TabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css/LocalStationScene.css" tabClosingPolicy="UNAVAILABLE" StackPane.alignment="CENTER"
                                 VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
                            <tabs>
                            </tabs>
                            <StackPane.margin>
                                <Insets />
                            </StackPane.margin>
                            <padding>
                                <Insets left="5.0" right="5.0" />
                            </padding>
                        </TabPane>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</StackPane>
